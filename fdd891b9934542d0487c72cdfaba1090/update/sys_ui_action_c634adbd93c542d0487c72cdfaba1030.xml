        <name>Create Bug</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[var workItem = new x_92180_ado.WorkItem();
var workItemType = "Bug";
var resultWorkNote = "";
var sys_id = current.sys_id;
var short_description = current.short_description;
var description = current.description;

workItem = workItem.create(workItemType,short_description, description, sys_id);

if (!global.JSUtil.notNil(workItem)) {
	resultWorkNote = "Error creating Azure "+workItemType;
	gs.addErrorMessage(resultWorkNote);
} else {
	resultWorkNote = "Azure " +workItemType + " created with url: "+JSON.parse(workItem)._links.html.href;
	gs.addInfoMessage(resultWorkNote);
}

current.work_notes = resultWorkNote;
current.update();
action.setRedirectURL(current);]]></script>
